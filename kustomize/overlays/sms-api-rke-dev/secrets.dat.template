# Template for secrets.dat
# Copy this file to secrets.dat and fill in your actual values
# DO NOT commit secrets.dat to git

# PostgreSQL database credentials (CNPG cluster)
POSTGRES_USER=sms
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DATABASE=sms
POSTGRES_HOST=sms-postgres-cluster-rw.postgres-cluster.svc.cluster.local
POSTGRES_PORT=5432

# GitHub credentials for container registry access
GH_USER_NAME=your_github_username
GH_USER_EMAIL=your_email@example.com
GH_PAT=ghp_YOURPERSONALACCESSTOKEN

# SSH key files for accessing HPC/Slurm cluster
SSH_PRIV_KEY_FILE=/path/to/your/ssh/private/key
SSH_PUB_KEY_FILE=/path/to/your/ssh/public/key
