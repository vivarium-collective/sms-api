              "title": "Detail"
            }
          },
          "type": "object",
          "title": "HTTPValidationError"
        },
        "SimulationRun": {
          "properties": {
            "id": {
              "type": "string",
              "title": "Id"
            },
            "last_updated": {
              "type": "string",
              "title": "Last Updated"
            },
            "_status": {
              "type": "string",
              "title": "Status",
              "default": "SUBMITTED"
            },
            "_data": {
              "additionalProperties": true,
              "type": "object",
              "title": "Data"
            },
            "protocol": {
              "type": "string",
              "title": "Protocol",
              "default": "secure"
            }
          },
          "type": "object",
          "required": [
            "id",
            "last_updated"
          ],
          "title": "SimulationRun"
        },
        "ValidationError": {
          "properties": {
            "loc": {
              "items": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "integer"
                  }
                ]
              },
              "type": "array",
              "title": "Location"
            },
            "msg": {
              "type": "string",
              "title": "Message"
            },
            "type": {
              "type": "string",
              "title": "Error Type"
            }
          },
          "type": "object",
          "required": [
            "loc",
            "msg",
            "type"
          ],
          "title": "ValidationError"
        },
        "VivariumDocument": {
          "properties": {
            "state": {
              "additionalProperties": true,
              "type": "object",
              "title": "State"
            },
            "composition": {
              "type": "string",
              "title": "Composition",
              "default": ""
            }
          },
          "type": "object",
          "title": "VivariumDocument"
        },
        "VivariumMetadata": {
          "properties": {
