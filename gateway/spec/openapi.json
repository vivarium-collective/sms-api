            },
            "location": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "title": "Location"
            },
            "secure": {
              "type": "boolean",
              "title": "Secure",
              "default": true
            }
          },
          "type": "object",
          "required": [
            "vivarium_id"
          ],
          "title": "VivariumMetadata"
        }
